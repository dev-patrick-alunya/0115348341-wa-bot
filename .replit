modules = ["nodejs-20", "nix"]
hidden = [".config", "package-lock.json"]
run = "npm  start"

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_05"

[unitTest]
language = "nodejs"

[deployment]
run = ["sh", "-c", "npm  start"]
deploymentTarget = "cloudrun"
ignorePorts = false

[workflows]

[[workflows.workflow]]
name = "Run Bot"
mode = "sequential"
author = 40085872

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node index.js"
